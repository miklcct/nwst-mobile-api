{
    "name" : "miklcct/nwst-mobile-api",
    "description" : "This is a PHP wrapper to call the Citybus / NWFB mobile API",
    "type" : "library",
    "license" : "Apache-2.0",
    "autoload" : {
        "psr-4" : {
            "Miklcct\\Nwst\\" : "src/"
        },
        "files" : ["src/helpers.php"]
    },
    "autoload-dev" : {
        "psr-4" : {
            "Miklcct\\Nwst\\Test\\" : "test/"
        }
    },
    "require" : {
        "php" : ">=7.3",
        "guzzlehttp/guzzle": "^6.5",
        "ext-json" : "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    }
}
